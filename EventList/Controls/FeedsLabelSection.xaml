<?xml version="1.0" encoding="UTF-8"?>
<ContentView 
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    x:Class="EventList.FeedsLabelSection">
	<ContentView.Padding>
        <OnPlatform x:TypeArguments="Thickness" Android="16,12,16,4" iOS="16,12,16,4"/>
    </ContentView.Padding>
    <ContentView.Content>
         <Label 
            HorizontalOptions="CenterAndExpand"
             FontSize="Medium" FontAttributes="Bold"
            x:Name="Section" TextColor="White">
        </Label>            
    </ContentView.Content>
</ContentView>
